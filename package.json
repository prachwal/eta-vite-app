    "svelte-check": "4.3.1",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "vite": "7.1.3"
  },
  "dependencies": {
    "@types/express": "5.0.3",
    "@vercel/node": "5.3.13",
    "express": "5.1.0"
  }
